{
  "name": "ai-proxy",
  "version": "2.4.0",
  "description": "High-performance AI API proxy with circuit breaker, rate limiting, adaptive concurrency, and real-time dashboard",
  "main": "proxy.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/RicherTunes/ai-proxy.git"
  },
  "homepage": "https://github.com/RicherTunes/ai-proxy#readme",
  "bugs": {
    "url": "https://github.com/RicherTunes/ai-proxy/issues"
  },
  "scripts": {
    "start": "node proxy.js",
    "start:single": "NO_CLUSTER=1 node proxy.js",
    "start:pm2": "pm2 start ecosystem.config.js",
    "stop:pm2": "pm2 stop glm-proxy",
    "restart:pm2": "pm2 restart glm-proxy",
    "logs:pm2": "pm2 logs glm-proxy",
    "status:pm2": "pm2 status glm-proxy",
    "monitor": "node monitor.js",
    "test": "jest --coverage",
    "test:unit": "jest --coverage=false",
    "test:ci": "jest --coverage",
    "test:quarantine": "jest --config jest.quarantine.config.js --no-coverage",
    "check:quarantine": "node scripts/check-quarantine-deadlines.js",
    "test:stress": "jest --config jest.stress.config.js --no-coverage",
    "test:all": "npm run test && npm run test:quarantine && npm run test:stress && npm run test:e2e",
    "test:watch": "jest --watch",
    "test:verbose": "jest --verbose --coverage",
    "test:e2e": "playwright test",
    "test:e2e:golden": "playwright test golden-semantics",
    "test:e2e:schema": "playwright test schema-snapshots",
    "test:e2e:perf-smoke": "playwright test dashboard-perf-smoke",
    "test:e2e:perf-heavy": "RUN_HEAVY_PERF=1 playwright test dashboard-perf-heavy",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:debug": "playwright test --debug",
    "screenshots:generate": "playwright test test/e2e/dashboard-docs.e2e.spec.js --update-snapshots",
    "screenshots:extract": "node scripts/extract-docs-screenshots.js",
    "stub:start": "node scripts/stub-upstream.js",
    "load": "node scripts/load.js",
    "load:smoke": "node scripts/load.js --rps=5 --duration=60 --warmup=5 --output=test-results/load-smoke.json",
    "load:medium": "node scripts/load.js --rps=20 --duration=120 --warmup=10 --output=test-results/load-medium.json",
    "load:heavy": "node scripts/load.js --rps=50 --duration=300 --warmup=30 --output=test-results/load-heavy.json",
    "load:ci": "npm run load:smoke",
    "baseline:smoke": "node scripts/validate-baseline.js smoke",
    "baseline:standard": "node scripts/validate-baseline.js standard",
    "baseline:stress": "node scripts/validate-baseline.js stress",
    "baseline:ci": "node scripts/validate-baseline.js smoke --output=test-results/baseline-smoke.json",
    "soak": "node scripts/soak.js",
    "soak:smoke": "node scripts/soak.js --duration=60 --warmup=30 --rps=10",
    "soak:short": "node scripts/soak.js --duration=300 --warmup=60 --rps=20",
    "soak:medium": "node scripts/soak.js --duration=1800 --warmup=300 --rps=20",
    "soak:long": "node scripts/soak.js --duration=7200 --warmup=600 --rps=20",
    "version": "node scripts/version.js",
    "commitlint": "commitlint --edit",
    "prepare": "husky",
    "release": "node scripts/release.js"
  },
  "keywords": [
    "proxy",
    "api",
    "circuit-breaker",
    "rate-limiting"
  ],
  "author": "RicherTunes",
  "license": "MIT",
  "devDependencies": {
    "@commitlint/cli": "^20.4.2",
    "@commitlint/config-conventional": "^20.4.2",
    "@playwright/test": "^1.48.0",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/exec": "^7.1.0",
    "@semantic-release/git": "^10.0.1",
    "eventsource": "^4.1.0",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "jsdom": "^24.1.3",
    "semantic-release": "^25.0.3"
  },
  "jest": {
    "testEnvironment": "node",
    "coverageDirectory": "coverage",
    "collectCoverageFrom": [
      "lib/**/*.js",
      "!lib/index.js"
    ],
    "coveragePathIgnorePatterns": [
      "lib/tray-icons.js",
      "lib/proxy-server.js"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 78,
        "functions": 85,
        "lines": 82,
        "statements": 82
      }
    },
    "testRegex": "test/.*\\.test\\.js$",
    "testPathIgnorePatterns": [
      "node_modules",
      "plugins",
      "test/stress"
    ],
    "verbose": true,
    "testTimeout": 30000
  }
}
